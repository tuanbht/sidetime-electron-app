{
  "name": "sidetime_app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.4",
    "@types/twilio-video": "^2.7.2",
    "axios": "^0.21.0",
    "electron-is-dev": "^1.2.0",
    "mobx": "^6.0.4",
    "mobx-persist": "^0.4.1",
    "mobx-react-lite": "^3.1.6",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "styled-components": "^5.2.1",
    "twilio": "^3.54.0",
    "twilio-video": "^2.9.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "tsc": "tsc --skipLibCheck",
    "format": "prettier --write '**/*.{ts,js,tsx,jsx,css,json,md,html,yml}' --ignore-path .gitignore",
    "electron": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack:linux": "yarn build",
    "preelectron-pack:windows": "yarn build",
    "preelectron-pack:macos": "yarn build",
    "electron-pack:linux": "electron-builder -l",
    "electron-pack:windows": "electron-builder -w",
    "electron-pack:macos": "electron-builder -m"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.3.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.1",
    "wait-on": "^5.2.0"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "author": {
    "name": "StartupLandia LLC",
    "email": "hello@startuplandia.io",
    "url": "https://www.startuplandia.io"
  },
  "build": {
    "appId": "com.startuplandia.sidetime_app",
    "productName": "SideTime",
    "copyright": "Copyright Â© 2020 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "protocols": {
      "name": "sidetime-deep-linking",
      "schemes": [
        "sidetime"
      ]
    }
  }
}
